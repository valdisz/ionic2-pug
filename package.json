{
  "name": "ionic-hello-world",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "build:pug": "gulp",
    "watch:pug": "gulp watch",
    "clean": "gulp clean",
    "build:ionic": "node ./build/build.js build",
    "watch:ionic": "ionic-app-scripts watch",
    "build": "npm run build-pug && npm run build-ionic",
    "watch": "npm-run-all --parallel watch:*",
    "serve:before": "watch",
    "emulate:before": "build",
    "deploy:before": "build",
    "build:before": "build",
    "run:before": "build"
  },
  "dependencies": {
    "ionic-angular": "^2.0.0-rc.0",
    "ionicons": "^3.0.0",
    "@ionic/storage": "^1.0.3",
    "ionic-native": "^2.0.3"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^0.0.30",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-pug": "^3.1.0",
    "gulp-watch": "^4.3.10",
    "npm-run-all": "^3.1.0",
    "typescript": "^2.0.3"
  },
  "description": "rcapp: An Ionic project",
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": []
}
